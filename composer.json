{
  "name": "quantimodo/wp",
  "license": "Private",
  "require": {
    "WordPress/WordPress": "3.9.3",
    "wpackagist-plugin/bugsnag": "1.2.0",
    "wpackagist-plugin/wp-crontrol": "1.3",
    "wpackagist-plugin/quick-pagepost-redirect-plugin": "5.1.8",
    "wpackagist-plugin/sendgrid-email-delivery-simplified": "1.9.4",
    "zendframework/zend-eventmanager": "3.0.1"
  },
  "require-dev": {
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "WordPress/WordPress",
        "type": "webroot",
        "version": "3.9.3",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/3.9.2.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "~1.1"
        }
      }
    },
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/Abolitionist-Project/QuantiModo-WordPress-Plugin.git"
    }
  ],
  "extra": {
    "webroot-dir": "wp",
    "webroot-package": "WordPress/WordPress",
    "installer-paths": {
      "wp/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugins"],
      "wp/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wp/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "autoload": {},
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "pre-install-cmd": [],
    "post-install-cmd": [],
    "post-update-cmd": []
  }
}
